    "@tinymce/tinymce-react": "^3.12.6",
    "autosuggest-highlight": "^3.1.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.6",
    "storybook-addon-material-ui5": "^1.0.0",
    "tar": "^6.1.6"
  },
